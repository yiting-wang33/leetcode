class Solution {
    public boolean isPalindrome(int x) {
        if (x<0 || x>0&&x%10==0) return false;
        if (x>=0 && x<10) return true;
        int re = 0;
        while (x>re) {
            re = re * 10 + x % 10;
            x/=10;
        }
        if (x==re || x==re/10) return true;
        else return false;
    }
}
